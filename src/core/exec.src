exec = function
  return function(input, fromPrompt = false)
    execCmd = function(cmd, args)
      res = null
      if typeof(@cmd) == "function" then
        res = cmd(args)
      else if cmd isa gsh.Cli then
        res = cmd.exec(args)
      end if
      return res
    end function

    if not input isa list or not input then
      if not fromPrompt then gsh.core.promptState.prompt
    else
      cmd = input[0]
      args = input[1 : ]
      if gsh.commands.hasIndex(cmd) then
        execCmd(gsh.commands[cmd], args)
      else
        get_shell.launch(cmd, args.join(" "))
      end if
    end if
  end function
end function